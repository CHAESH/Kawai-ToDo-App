    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "co.chaesh.kawai"
    },
    "githubUrl": "https://github.com/CHAESH/Kawai-ToDo-App"
